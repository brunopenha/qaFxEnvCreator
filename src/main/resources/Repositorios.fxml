<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.VBox?>


<AnchorPane xmlns:fx="http://javafx.com/fxml" prefHeight="200.0" prefWidth="200.0"
	BorderPane.alignment="CENTER" fx:id="arquivosAnchor" fx:controller="br.nom.penha.bruno.qafxenvcreator.controle.ArquivosFXMLController">
	<children>
		<VBox id="vBox" layoutX="146.0" layoutY="102.0"
			prefHeight="366.0" prefWidth="535.0">
			<padding>
				<Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
			</padding>
			<children>
				<Label id="repLabel" prefHeight="18.0" prefWidth="449.0"
					text="Insert the path where you did clone your project from Git repository:" />
				<TextField id="repLocationTextField" />
				
				<Button mnemonicParsing="false" text="Start" fx:id="okBtn" onAction="#buscaArquivosAction"/>
				
				<TextArea id="outputTextArea" prefHeight="123.0"
					prefWidth="515.0" />
			</children>
		</VBox>
	</children>
</AnchorPane>

